		"globals": "^15.12.0",
		"husky": "^9.1.7",
		"mdsvex": "^0.11.2",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.2.7",
		"svelte-check": "^4.1.0",
		"tailwindcss": "^4.0.0-beta.2",
